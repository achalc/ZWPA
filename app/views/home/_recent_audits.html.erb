<table class="table">
  <tr>
    <th width="200">Client</th>
    <th width="120">Generator</th>
    <th width="120">Start Date</th>
    <th width="120">End Date</th>
  </tr>

  <% audits.each do |audit| %>
    <tr>
      <!-- How do I get the company name of request??? -->
      <td><%= link_to audit.request.customer.company_name, audit_path(audit) %></td>
      <td><%= audit.generator%></td>
      <td><%= audit.start_date.strftime('%b %e, %Y')%></td>
      <td><%= audit.end_date.strftime('%b %e, %Y')%></td>
      
      
    </tr>
  <% end %>
</table></br>

<%= link_to "View All Audits", audits_path %>

